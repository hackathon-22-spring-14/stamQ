{
  "name": "hackathon-22-spring-14-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "tsc --noEmit && vite --mode=development",
    "build": "tsc --noEmit && vite build --mode=production",
    "deploy": "gh-pages -d docs"  ,
    "serve": "vite preview",
    "preview": "vite preview",
    "lint": "eslint --fix src/**/*.{ts,vue} && eslint --fix src/**/*.{ts,vue}",
    "format": "prettier -w src/**/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "fix": "yarn format && yarn lint",
    "gen-api": "node ./scripts/generate-apis.js",
    "homepage": "./",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-plugin-vue": "^9.1.1",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.1.2",
    "ts-morph": "^15.1.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  }
}
